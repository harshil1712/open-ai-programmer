{
  "name": "open-ai-programmer",
  "version": "1.0.0",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && prisma generate && RWSDK_DEPLOY=1 npm run build && wrangler deploy",
    "migrate:dev": "prisma generate && wrangler d1 migrations apply DB --local",
    "migrate:prd": "wrangler d1 migrations apply DB --remote",
    "migrate:new": "rw-scripts migrate-new",
    "generate": "rw-scripts ensure-env && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "prepare": "vibe-rules install cursor"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.15",
    "@ai-sdk/react": "^2.0.15",
    "@cloudflare/sandbox": "^0.3.0",
    "@prisma/adapter-d1": "~6.8.2",
    "@prisma/client": "~6.8.2",
    "@tailwindcss/vite": "^4.1.12",
    "ai": "^5.0.15",
    "harden-react-markdown": "^1.0.4",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "rwsdk": "0.1.35",
    "tailwindcss": "^4.1.12",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.11.5",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "prisma": "~6.8.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "vibe-rules": "^0.2.31",
    "vite": "^7.1.2",
    "wrangler": "^4.30.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  }
}
